.navbar-header {
    width: 640px;
    position: relative;
    /*margin-top: 50px;*/
    border-bottom: 1px solid #d5d5d5;
}

.navbar-header h3 {
    color: #262626;
    text-align: center;
    line-height: 68px;
    font-size: 24px;
    background: #fff;
}

.navbar-back {
    background: url("../image/return.png") no-repeat left center;
    position: absolute;
    left: 20px;
    top: 50%;
    margin-top: -1.5rem;
    width: 2rem;
    height: 3rem;
    font-size: 3rem;
}

/* 标题 */
.tittle {
    width: 640px;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    z-index: 98;
    overflow: hidden;
    margin: 0 auto;
    height: 70px;
    background: #5683e1;
}

.tittle img {
    position: absolute;
    left: 18px;
    top: 20px;
    width: 17px;
    height: auto;
}

.tittle h2 {
    text-align: center;
    font-size: 24px;
    color: #ffffff;
}

/*----------------------------------------ͷ������ ending----------------------------------*/
/* 更新时间样式 */
p.under {
    width: 100%;
    text-align: center;
    font-size: 20px;
    line-height: 22px;
    color: #818181;
    margin-bottom:5px;
}

/*----------------------------------------�������----------------------------------------*/

.dateTools {
    position: relative;
    overflow: hidden;
    width: 640px;
    height: 65px;
    line-height: 76px;
    text-align: center;
    background: #f9f9f9;
    margin-top: 75px;
}

.dateTools img {
    width: 100%;
    max-width: 32px;
    margin-left: 5px;
    margin-top: -5px;
}

.dateTools ul {
    width: 24%;
    margin: 0 auto;
    border-radius: 3px;
}

.dateTools li {
    width: 100%;
    text-align: center;
    border-radius: 3px;
    line-height: 40px;
}

.dateTools li i.day_choose {
    display: block;
    width: 87px;
    font-size: 24px;
    border-radius: 3px;
    background: #e7e7e7;
}

.dateTools li i.before {
    margin-left: 49px;
}

.dateTools li i.after {
    margin-left: 17px;
}

.dateTools li i.gray {
    color: gray;
}

.dateTools .day_left {
    background: url(../image/up.png) no-repeat 20px 24px;
}

.dateTools .day_right {
    background: url(../image/down.png) no-repeat 123px 24px;
}

#ticketHisDate {
    width: 320px;
    height: 58px;
    font-family: Arial, sans-serif;
    font-size: 28px;
    text-align: center;
    background: #fff;
    border-radius: 3px;
    line-height: 58px;
    min-width: 145px;
}

#ticketHisDate li {
    background: #fff;
}

#ticketHisDate li i {
    margin-left: 5px;
    font-family: "STXihei", "Microsoft YaHei", sans-serif;
}

#ticketHisDate li i:after {
    content: "|";
    margin-left: 10px;
    color: #c8c8c8;
}

/*----------------------------------------������� ending----------------------------------*/
