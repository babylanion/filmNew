ul, li, h3, i, p {
    margin: 0;
    padding: 0;
}

ul {
    display: inline-block;
}

li {
    list-style: none;
    display: inline-block;
}

i {
    font-style: normal;
}

img {
    border: none;
}

a {
    color: #ffffff;
}

body {
    font-family: "STXihei", "Microsoft YaHei", sans-serif;
    font-size: 62.5%;
    max-width: 640px;
    margin: 0 auto;
    /*font-size: 20px;*/
}

.wrap {
    background: #f9f9f9;
}

/*----------------------------------------����logo---------------------------*/
header {
    width: 640px;
    margin: 0 auto;
    background: #5683e1;
}

.table > thead > tr > th {
    line-height: 50px;
}

.logo {
    width: 640px;
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 70px;
    z-index: 99;
    text-align: center;
}
#tittle{
    display: block;
    line-height: 70px;
    color: #ffffff;
    font-size: 24px;
    text-indent: 32px;
    background: url(../image/logo.png) no-repeat 208px center/26px auto
}
